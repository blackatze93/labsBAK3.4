{% extends '@EasyAdmin/default/layout.html.twig' %}

{% block head_stylesheets %}
    {{ parent() }}
    <link rel="stylesheet" href="{{ asset('build/app.css') }}">
    <link rel="stylesheet" href="{{ asset('build/datetimepicker.css') }}">
    <link rel="stylesheet" href="{{ asset('build/noty.css') }}">
{% endblock %}

{% block head_javascript %}
    {{ parent() }}
    <script src="{{ asset('build/app.js') }}"></script>
    <script src="{{ asset('build/datetimepicker.js') }}"></script>
    <script src="{{ asset('build/noty.js') }}"></script>
{% endblock %}